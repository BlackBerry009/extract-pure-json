{
  "name": "extract-pure-json",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "extract",
    "json"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackBerry009/extract-pure-json"
  },
  "homepage": "https://github.com/BlackBerry009/extract-pure-json.git",
  "bugs": "https://github.com/BlackBerry009/extract-pure-json/issues",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build": "tsc",
    "test": "c8 ava",
    "prepublishOnly": "npm run build",
    "commit": "git-cz",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "dependencies": {
    "@types/node": "^18.15.5",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "git-cz": "^4.9.0",
    "ts-node": "^10.9.1"
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ],
    "files": [
      "test/**/*.ts"
    ]
  },
  "author": "BlackBerry",
  "license": "ISC"
}
